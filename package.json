    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-tailwind-purgecss": "0.2.0",
    "vitest": "^1.1.0",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "firebase-admin": "^12.0.0",
